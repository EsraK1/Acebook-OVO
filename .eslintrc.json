        "commonjs": true,
        "es6": true,
        "node": true,
        "jest/globals": true,
        "cypress/globals": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended",
      "plugin:cypress/recommended"
    ],
    "globals": {
